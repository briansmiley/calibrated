{
  "name": "estimation-game",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.26.1",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp",
      "unrs-resolver"
    ]
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "db:seed": "tsx scripts/seed.ts",
    "db:types": "supabase gen types typescript --project-id zxaxpoerzsgeomaqjbsc > src/types/database.ts && echo '\n// Convenience type exports\nexport type UnitType = Database[\"public\"][\"Enums\"][\"unit_type\"]\nexport type Question = Database[\"public\"][\"Tables\"][\"questions\"][\"Row\"]\nexport type QuestionInsert = Database[\"public\"][\"Tables\"][\"questions\"][\"Insert\"]\nexport type Guess = Database[\"public\"][\"Tables\"][\"guesses\"][\"Row\"]\nexport type GuessInsert = Database[\"public\"][\"Tables\"][\"guesses\"][\"Insert\"]\nexport type Profile = Database[\"public\"][\"Tables\"][\"profiles\"][\"Row\"]\nexport type ProfileUpdate = Database[\"public\"][\"Tables\"][\"profiles\"][\"Update\"]' >> src/types/database.ts",
    "test": "pnpm run test:e2e",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:report": "playwright show-report"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.91.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "discord-interactions": "^4.4.0",
    "lucide-react": "^0.562.0",
    "next": "16.1.4",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-icons": "^5.5.0",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.4",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}
